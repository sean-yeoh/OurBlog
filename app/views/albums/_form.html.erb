<%= form_for album do |form| %>
   <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: "form-control" %>
  </div>

   <div class="form-group">
    <%= form.label :photos %>
    <%= form.file_field :photos, multiple: true %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>